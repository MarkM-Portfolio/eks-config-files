<config id="searchConfig" version="18" buildlevel="@BUILD_LEVEL@"
	xmlns="http://www.ibm.com/LotusConnectionsSearch" xmlns:tns="http://www.ibm.com/LotusConnectionsSearch" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ibm.com/LotusConnectionsSearch search-config.xsd">

	<indexingSchedule />

 	<indexSettings maxIndexerThreads="1" location="${SEARCH_INDEX_DIR}" allowResumption="true"/>

	<crawlerSettings persistenceLocation="${CRAWLER_PAGE_PERSISTENCE_DIR}"
		pageSize="500" maxCrawlerThreads="3" />

	<displayTempIndexingDifficultiesMsg enabled="false" />
	
	<languageSupport defaultLocale="en">
		<dictionary locale="en" path="${SEARCH_DICTIONARY_DIR}" />
	</languageSupport>

	<services>
		<connService name="activities" contentSource="activitiesContentSource" />
		<connService name="calendar" contentSource="calendarContentSource" />
		<connService name="ecm_files" contentSource="ecmFilesContentSource" />
		<connService name="people_finder" contentSource="peopleFinderContentSource" includeInAllConfigured="true"/>
	</services>


	<contentSources>
		<seedlist name="activitiesContentSource" pageSize="500" />
		<seedlist name="calendarContentSource" seedlistLocation="calendar/seedlist/myserver" />
		<seedlist name="ecmFilesContentSource" seedlistLocation="atom/seedlist/myserver" />
		<seedlist name="peopleFinderContentSource" providingService="profiles" />
		<seedlist name="profilesContentSource" pageSize="100" />		
	</contentSources>

