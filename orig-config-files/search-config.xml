	<boostSettings>
		<boost name="content">
		    <fieldBoost>
		        <field name="title" boost="3.0" />
		        <field name="tag" boost="2.0" /> 
		    </fieldBoost>
		    <recencyBoost>
			 	<param name="alpha" value="0.0005"/> 
			</recencyBoost>
		</boost>
		<boost name="contentSolr">
		    <fieldBoost>
		        <field name="title" boost="3.0" />
		        <field name="tag" boost="2.0" /> 
		    </fieldBoost>
		    <recencyBoost>
			 	<param name="a" value="0.09"/> 
